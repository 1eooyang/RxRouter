<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="zlc.season.sample.UriMenuActivity">

    <Button
        android:id="@+id/current_uri"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="当前Module class跳转" />

    <Button
        android:id="@+id/current_uri_for_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="当前Module class跳转并获取返回结果" />

    <Button
        android:id="@+id/library_uri"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Library Module class跳转" />

    <Button
        android:id="@+id/library_uri_for_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Library Module class跳转并获取返回结果" />
</LinearLayout>
