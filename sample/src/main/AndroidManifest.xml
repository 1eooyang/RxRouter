<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zlc.season.sample">

    <application
        android:name=".CustomApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UriActivity"
            android:label="UriActivity" />
        <activity
            android:name=".UriForResultActivity"
            android:label="UriForResultActivity" />
        <activity
            android:name=".ActionActivity"
            android:label="ActionActivity">
            <intent-filter>
                <action android:name="zlc.season.sample.action" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActionForResultActivity"
            android:label="ActionForResultActivity">
            <intent-filter>
                <action android:name="zlc.season.sample.action_for_result" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ClassActivity"
            android:label="ClassActivity" />
        <activity
            android:name=".ClassForResultActivity"
            android:label="ClassForResultActivity" />
        <activity
            android:name=".UriMenuActivity"
            android:label="UriMenuActivity" />
        <activity
            android:name=".ActionMenuActivity"
            android:label="ActionMenuActivity" />
        <activity
            android:name=".ClassMenuActivity"
            android:label="ClassMenuActivity" />
        <activity
            android:name=".AndroidActionMenuActivity"
            android:label="AndroidActionMenuActivity" />
        <activity android:name=".FirewallMenuActivity"></activity>
    </application>

</manifest>